	<queryString>
		<![CDATA[SELECT bridging_surat_pri_bpjs.no_rawat, 
       bridging_surat_pri_bpjs.no_kartu, 
       reg_periksa.no_rkm_medis, 
       pasien.nm_pasien, 
       pasien.tgl_lahir, 
       pasien.jk,
       pasien.pekerjaan,
       pasien.alamat, 
       bridging_surat_pri_bpjs.diagnosa AS keterangan, 
       bridging_surat_pri_bpjs.tgl_surat AS tanggal_mulai, 
       bridging_surat_pri_bpjs.no_surat, 
       bridging_surat_pri_bpjs.tgl_rencana AS tanggal_selesai, 
       bridging_surat_pri_bpjs.kd_dokter_bpjs AS kode_dokter, 
       bridging_surat_pri_bpjs.nm_dokter_bpjs AS nama_dokter, 
       bridging_surat_pri_bpjs.kd_poli_bpjs AS kode_poli, 
       bridging_surat_pri_bpjs.nm_poli_bpjs AS nama_poli
FROM reg_periksa 
INNER JOIN bridging_surat_pri_bpjs ON bridging_surat_pri_bpjs.no_rawat = reg_periksa.no_rawat 
INNER JOIN pasien ON reg_periksa.no_rkm_medis = pasien.no_rkm_medis 

UNION ALL

SELECT surat_keterangan_covid.no_rawat, 
       '' AS no_kartu, 
       pasien.no_rkm_medis, 
       pasien.nm_pasien, 
       pasien.tgl_lahir, 
       pasien.jk, 
       surat_keterangan_covid.sehat AS keterangan, 
       surat_keterangan_covid.berlakumulai AS tanggal_mulai, 
       surat_keterangan_covid.no_surat, 
       surat_keterangan_covid.berlakuselsai AS tanggal_selesai, 
       surat_keterangan_covid.kd_dokter AS kode_dokter, 
       dokter.nm_dokter AS nama_dokter, 
       '' AS kode_poli, 
       spesialis.nm_sps AS nama_poli
FROM surat_keterangan_covid 
INNER JOIN reg_periksa ON surat_keterangan_covid.no_rawat = reg_periksa.no_rawat 
INNER JOIN pasien ON reg_periksa.no_rkm_medis = pasien.no_rkm_medis 
INNER JOIN dokter ON surat_keterangan_covid.kd_dokter = dokter.kd_dokter 
INNER JOIN spesialis ON dokter.kd_sps = spesialis.kd_sps]]>
	</queryString>
