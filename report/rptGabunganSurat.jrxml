	<queryString>
		<![CDATA[SELECT reg_periksa.no_rawat AS no_rawat, 
           pasien.no_rkm_medis AS no_rkm_medis, 
           pasien.nm_pasien AS nama_pasien, 
           pasien.tgl_lahir AS tanggal_lahir, 
           pasien.jk AS jenis_kelamin,
           pasien.pekerjaan AS pekerjaan, 
           bridging_surat_pri_bpjs.no_kartu AS no_kartu, 
           bridging_surat_pri_bpjs.diagnosa AS diagnosa_pri, 
           bridging_surat_pri_bpjs.tgl_surat AS tanggal_mulai_pri, 
           bridging_surat_pri_bpjs.tgl_rencana AS tanggal_selesai_pri, 
           bridging_surat_pri_bpjs.no_surat AS nomor_surat_pri, 
           bridging_surat_pri_bpjs.kd_dokter_bpjs AS kode_dokter_pri, 
           bridging_surat_pri_bpjs.nm_dokter_bpjs AS nama_dokter_pri,
           bridging_surat_pri_bpjs.kd_poli_bpjs AS kode_poli_bpjs,
           bridging_surat_pri_bpjs.nm_poli_bpjs AS nama_poli_bpjs, 
           surat_keterangan_covid.no_surat AS nomor_surat_covid, 
           surat_keterangan_covid.berlakumulai AS tanggal_mulai_covid, 
           surat_keterangan_covid.berlakuselsai AS tanggal_selesai_covid, 
           surat_keterangan_covid.kd_dokter AS kode_dokter_covid, 
           dokter.nm_dokter AS nama_dokter_covid, 
           surat_keterangan_covid.igm AS nilai_igm, 
           surat_keterangan_covid.igg AS nilai_igg, 
           surat_keterangan_covid.sehat AS sehat, 
           surat_keterangan_covid.tidaksehat AS tidak_sehat, 
           CONCAT(pasien.alamat, ', ', kelurahan.nm_kel, ', ', kecamatan.nm_kec, ', ', kabupaten.nm_kab) AS alamat 
    FROM reg_periksa 
    LEFT JOIN bridging_surat_pri_bpjs ON reg_periksa.no_rawat = bridging_surat_pri_bpjs.no_rawat 
    LEFT JOIN surat_keterangan_covid ON reg_periksa.no_rawat = surat_keterangan_covid.no_rawat 
    LEFT JOIN pasien ON reg_periksa.no_rkm_medis = pasien.no_rkm_medis 
    LEFT JOIN dokter ON surat_keterangan_covid.kd_dokter = dokter.kd_dokter 
    LEFT JOIN kelurahan ON pasien.kd_kel = kelurahan.kd_kel 
    LEFT JOIN kecamatan ON pasien.kd_kec = kecamatan.kd_kec 
    LEFT JOIN kabupaten ON pasien.kd_kab = kabupaten.kd_kab
    WHERE reg_periksa.no_rawat = $P{no_rawat}]]>
	</queryString>
